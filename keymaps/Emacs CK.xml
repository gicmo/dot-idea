<keymap version="1" name="Emacs CK" parent="Emacs">
  <action id="DashLauncherAction">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="d" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="g" />
  </action>
  <action id="RecentFiles">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="r" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl r" />
  </action>
  <action id="SelectAllOccurrences">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift ctrl comma" />
  </action>
  <action id="SelectNextOccurrence">
    <keyboard-shortcut first-keystroke="alt j" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="shift period" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
</keymap>